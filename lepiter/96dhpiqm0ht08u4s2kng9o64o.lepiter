				"paragraphStyle" : {
					"__type" : "textStyle"
				},
				"string" : "###efficient object representation"
			},
			{
				"__type" : "textSnippet",
				"children" : {
					"__type" : "snippets",
					"items" : [ ]
				},
				"createEmail" : {
